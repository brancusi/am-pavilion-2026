{
  "name": "amp",
  "scripts": {
    "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
    "build-report": "npx shadow-cljs run shadow.cljs.build-report app target/build-report.html",
    "postcss:dev": "cross-env TAILWIND_MODE=watch postcss resources/css/tailwind.css -o ./public/css/main.css --verbose -w",
    "fonts:dev": "cp -rf ./resources/fonts ./public/",
    "images:dev": "cp -rf ./resources/images ./public/",
    "fonts:build": "cp -rf ./resources/fonts ./public/",
    "images:build": "cp -rf ./resources/images ./public/",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss resources/css/tailwind.css -o ./public/css/main.css --verbose",
    "shadow:release": "sh -c 'shadow-cljs release release --config-merge \"{:release-version \\\"$(git rev-parse --short HEAD)\\\"}\"'",
    "postcss:release": "cross-env NODE_ENV=production postcss resources/css/tailwind.css -o ./release/css/main.css --verbose",
    "fonts:release": "cp -rf ./resources/fonts ./release/",
    "images:release": "cp -rf ./resources/images ./release/",
    "misc:release": "cp -rf ./resources/misc/* ./release/",
    "misc:dev": "cp -rf ./resources/misc/* ./public/",
    "html:release": "node build/update-html.js",
    "release": "run-s *:release",
    "dev": "run-p -l *:dev",
    "build": "run-s *:build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "2.0.16",
    "@mux/mux-player-react": "^3.8.0",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.3.5",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "gsap": "^3.13.0",
    "hls.js": "^1.6.14",
    "netlify-cli": "^23.11.0",
    "platform": "1.3.5",
    "react": "^19.2.0",
    "react-dom": "19.2.0",
    "react-refresh": "^0.18.0",
    "react-svg": "16.3.0",
    "react-tracked": "^1.7.11",
    "scheduler": "^0.23.0",
    "shadow-cljs": "^3.2.1",
    "stack-trace": "0.0.10",
    "stacktrace-js": "2.0.2",
    "ws": "7.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "shadow-cljs": "^3.2.1",
    "tailwindcss": "^3.2.4"
  }
}
